@go.Package { name = "github.com/declix/pkl-declix/dpkg" }
module declix.dpkg

import "@go/go.pkl"
import ".../declix.pkl"

class Package extends declix.Resource {
    fixed type = "dpkg"
    fixed id = type + ":" + name

    name: String
    state: State
    content: declix.Content
}

typealias State = "installed" | "missing";